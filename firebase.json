{
  "emulators": {
    "auth": {
      "host": "127.0.0.1",
      "port": 9110
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8190
    },
    "storage": {
      "host": "127.0.0.1",
      "port": 9290
    },
    "hosting": {
      "host": "127.0.0.1",
      "port": 5010
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 5011
    },
    "ui": {
      "enabled": true,
      "host": "127.0.0.1",
      "port": 4001
    },
    "hub": {
      "host": "127.0.0.1",
      "port": 4410
    },
    "logging": {
      "host": "127.0.0.1",
      "port": 4510
    }
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
